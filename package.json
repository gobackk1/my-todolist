{
  "name": "todolist",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "npx jest",
    "start": "npx webpack-dev-server --open --hot --mode development",
    "build": "npx webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gobackk1/my-todolist.git"
  },
  "author": "kengo.kawabata",
  "bugs": {
    "url": "https://github.com/gobackk1/my-todolist/issues"
  },
  "homepage": "https://github.com/gobackk1/my-todolist#readme",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@types/enzyme": "^3.10.6",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.10.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "axios": "^0.20.0",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^2.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "firebase": "^7.20.0",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^26.4.2",
    "redux-devtools-extension": "^2.13.8",
    "terser-webpack-plugin": "^4.2.0",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2",
    "typescript-fsa": "^3.0.0",
    "typescript-fsa-reducers": "^1.2.2",
    "typescript-fsa-redux-thunk": "^2.10.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.8.2",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "eslintIgnore": [
    ".eslintrc.json"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:react-hooks/recommended"
    ],
    "plugins": [
      "react",
      "react-hooks",
      "@typescript-eslint"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module",
      "project": "tsconfig.json"
    },
    "rules": {
      "no-console": "off",
      "semi": "off"
    },
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [
            [
              "@",
              "./src"
            ],
            [
              "~redux",
              "./src/scripts/redux"
            ]
          ],
          "extensions": [
            ".ts",
            ".js",
            ".tsx",
            ".json"
          ]
        }
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 versions"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "babel-jest",
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(xxxx.*?\\.js$))"
    ],
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      ".ts",
      ".tsx",
      ".js",
      ".json"
    ],
    "moduleNameMapper": {
      "@(components|helper|redux|config|modules|style)?/(.*)": "<rootDir>/src/$1/$2"
    }
  }
}
