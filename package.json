{
  "name": "todolist",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test:integration": "npx jest -c ./test/integration/jest.config.json --runInBand --detectOpenHandles",
    "test:e2e": "npx jest -c ./test/e2e/jest.config.json --runInBand --detectOpenHandles",
    "test:firestore": "FIRESTORE_EMULATOR_HOST=localhost:8081 npx jest -c ./test/firestore/jest.config.json --detectOpenHandles",
    "start": "npx webpack-dev-server --open --hot --mode development",
    "build": "npx webpack --mode production",
    "deploy": "npx firebase deploy",
    "deploy:functions": "npx firebase deploy --only functions",
    "deploy:rules": "npx firebase deploy --only firestore:rules && npx firebase deploy --only storage:rules",
    "emulator:firestore": "npx firebase emulators:start --only firestore",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
