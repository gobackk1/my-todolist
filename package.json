  "eslintIgnore": [
    ".eslintrc.json",
    "./e2e",
    "*.spec.js"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 versions"
    ]
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.jest.json"
      }
    },
    "globalSetup": "./e2e/setup.js",
    "globalTeardown": "./e2e/teardown.js",
    "testEnvironment": "./e2e/puppeteer_environment.js",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "babel-jest",
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(xxxx.*?\\.js$))"
    ],
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1",
      "~redux(.*)$": "<rootDir>/src/scripts/redux/$1"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupEnzyme.ts"
    ]
  }
}
